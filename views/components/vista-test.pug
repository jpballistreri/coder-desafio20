table(class="table")
    thead
    tr
        th(scope="col") Nombre
        th(scope="col") Precio
        th(scope="col") Foto 
    tbody
        each producto in arrayProductos
            tr
            td #{producto.title}
            td U$S #{producto.price}
            td 
                img(src=producto.thumbnail)
        else
            h3 No hay productos